{
  "manifest_version": 2,
  "name": "CrappyClippy",
  "version": "0",
  "version_name": "paperclip",
  "description": "Manage tabs in Google Chrome",
  "icons": {
    "32": "img/icon32.png"
  },
  "browser_action": {
    "default_icon": {
      "32": "img/icon32.png"
    },
    "default_title": "Tab Buddy",
    "default_popup": "popup.html"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "incognito": "not_allowed",
  "permissions": [
    "activeTab",
    "background",
    "debugger",
    "management",
    "notifications",
    "storage",
    "system.cpu",
    "system.memory",
    "tabCapture",
    "tabs"
  ],
  "web_accessible_resources": [
    "css/*",
    "img/*",
    "js/*",
    "index.html",
    "popup.html"
  ]
}